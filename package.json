{
  "name": "lynx-scanner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typedoc": "typedoc",
    "prepare": "husky",
    "eslint": "eslint --fix .",
    "prettier": "prettier --write .",
    "stylelint:actions": "stylelint --fix \"src/**/*.{css,scss}\"",
    "convert-md-to-pdf": "yarn run md-to-pdf ./docs/*.md ./docs/**/*.md",
    "rtk:gen": "rimraf ./src/store/api/gen && mkdir ./src/store/api/gen && rtk-query-codegen-openapi openapi-config.ts && prettier -w ./src/store/api/gen",
    "postinstall": "yarn run rtk:gen"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "@sentry/react": "^9.28.1",
    "@sentry/tracing": "^7.114.0",
    "@yudiel/react-qr-scanner": "^2.2.1",
    "axios": "^1.8.2",
    "compromise": "^14.14.0",
    "formik": "^2.4.6",
    "i18next": "^24.2.2",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^3.0.2",
    "i18next-localstorage-backend": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-feather": "^2.0.10",
    "react-html5-camera-photo": "^1.5.11",
    "react-i18next": "^15.5.1",
    "react-qr-code": "^2.0.15",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-toolkit": "^1.1.2",
    "tailwind-merge": "^3.0.2",
    "tesseract.js": "^6.0.0",
    "theme-change": "^2.5.0",
    "use-file-picker": "^2.1.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@eslint/eslintrc": "^3.0.2",
    "@rtk-query/codegen-openapi": "^2.0.0",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.5",
    "@tsconfig/vite-react": "^3.4.0",
    "@types/i18next-browser-languagedetector": "^3.0.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-html5-camera-photo": "^1.5.3",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^8.31.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "base-x": "^5.0.1",
    "cssnano": "^7.0.6",
    "daisyui": "^5.0.27",
    "electron-log": "^5.1.7",
    "esbuild-runner": "^2.2.2",
    "eslint": "8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.0.11",
    "lint-staged": "^15.4.3",
    "md-to-pdf": "^5.2.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-actions-formatters": "^16.3.1",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-plugin-defensive-css": "^1.0.3",
    "tailwindcss": "^4.1.5",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-plugin-mdn-links": "^3.1.22",
    "typescript": "^5.4.5",
    "vite": "^6.2.1",
    "vite-plugin-bundlesize": "^0.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-mkcert": "^1.17.7",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-robots": "^1.0.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.{css,scss,js,jsx,ts,tsx})": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=23"
  }
}
